    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cake launch runner with args",
            "program": "bin/cake.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--tte", "True should be true"
            ],
        },
        {
            "name": "Run test basic - with args",
            "program": "test/test-basic.cake.dart",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--define=testSearchFor=True should be true"]
        },
        {
            "name": "Run group simple - with args",
            "program": "test/test-group.cake.dart",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--define=groupSearchFor=Should run all children tests"]
        },
        {
            "name": "Run group complex - with args",
            "program": "test/test-context.cake.dart",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--define=groupSearchFor=Group with same context passes context to children"]
        }
    ]
}
